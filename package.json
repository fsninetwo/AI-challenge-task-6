{
  "name": "enigma-machine",
  "version": "1.0.0",
  "description": "Enigma machine simulator with comprehensive test suite",
  "main": "enigma.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "node enigma.js"
  },
  "keywords": [
    "enigma",
    "encryption",
    "cryptography",
    "simulator"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "enigma.js"
    ],
    "testMatch": [
      "**/*.test.js"
    ]
  }
} 